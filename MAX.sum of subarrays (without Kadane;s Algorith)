INPUT :   9
        -2 1 -3 4 -1 2 1 -5 4
OUTPUT:   6
-----------------------------------------------------

// Time Complexity : O(n3)


import java.util.*;
class Test{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
       int n=s.nextInt();
       int a[]=new int[n];
       for(int i=0;i<n;i++){
           a[i]=s.nextInt();
       }
       int max=0;
       
       for(int i=0;i<n;i++){
           for(int j=i;j<n;j++){
               int sum=0;
               for(int k=i;k<=j;k++){
                   sum=sum+a[k];
               }
               if(max<sum)
               max=sum;
           }
       }
       System.out.println(max);
    }
}
