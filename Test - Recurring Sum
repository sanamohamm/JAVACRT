INPUT:
        5
        1 4 2 6 3
OUTPUT:
        1 5 7 13 16
-------------------------------------------------------------------------------

import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        int sum=0;
        for(int i=0;i<n;i++){
            sum+=a[i];
            System.out.print(sum+" ");
        }

    }
}
