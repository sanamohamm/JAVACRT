INPUT:    4
          1 4 7 2
OUTPUT:   7
--------------------------------------------------------------------------

import java.util.Scanner;
class TestClass {
    public static void main(String args[] ) throws Exception {
     Scanner s=new Scanner(System.in);
     int n=s.nextInt();
     int a[]=new int[n];
     for(int i=0;i<n;i++){
        a[i]=s.nextInt();
     }
     int max=a[0];
     for(int i=1;i<n;i++){
        if(a[i]>max)
        max=a[i];
     }
        System.out.println(max);
    }
}
